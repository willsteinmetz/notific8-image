extends ../../bower_components/notific8/src/pug/demo.pug

block notific8DemoTitle
  h1 notific8 demo - image module

block lowerConfig
  fieldset
    legend Image module settings
    .input-field.row
      .col.s4
        label Image (optional) - <i>provided by module</i>
      .col.s8
        div(style='display: flex;').notific8-image-options
          input.with-gap#notific8ImageNone(type='radio', name='notific8Image', checked='', value='')
          label(for='notific8ImageNone') None &nbsp;
          label(for='notific8ImageGithub').notific8-image-option
            input#notific8ImageGithub(type='radio', name='notific8Image', value='data:image/svg+xml;base64,')
            | &nbsp;
            img(src='data:image/svg+xml;base64,', style="width: 40px; height: 40px;", alt='mark-github')
            | &nbsp;
          label(for='notific8ImageMarkdown').notific8-image-option
            input#notific8ImageMarkdown(type='radio', name='notific8Image', value='data:image/svg+xml;base64,')
            | &nbsp;
            img(src='data:image/svg+xml;base64,', style="width: 40px; height: 40px;", alt='markdown')
            | &nbsp;
          label(for='notific8ImagePaint').notific8-image-option
            input#notific8ImagePaint(type='radio', name='notific8Image', value='data:image/svg+xml;base64,')
            | &nbsp;
            img(src='data:image/svg+xml;base64,', style="width: 40px; height: 40px;", alt='paintcan')
            | &nbsp;
          label(for='notific8ImageWarning').notific8-image-option
            input#notific8ImageWarning(type='radio', name='notific8Image', value='data:image/svg+xml;base64,')
            | &nbsp;
            img(src='data:image/svg+xml;base64,', style="width: 40px; height: 40px;", alt='issue-opened')
            | &nbsp;
          label(for='notific8ImageSquirrel').notific8-image-option
            input#notific8ImageSquirrel(type='radio', name='notific8Image', value='data:image/svg+xml;base64,')
            | &nbsp;
            img(src='data:image/svg+xml;base64,', style="width: 40px; height: 40px;", alt='squirrel')
        p Images are SVG files that are part of GitHub's <a href="https://octicons.github.com/", target="_blank">Octicon</a> icon set
